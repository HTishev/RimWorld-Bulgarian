  <!-- EN: Draft -->
  <CommandDraftLabel>На бой!</CommandDraftLabel>
  <!-- EN: Undraft -->
  <CommandUndraftLabel>Разпускане</CommandUndraftLabel>
  <!-- EN: Toggle military draft.\n\nDrafted people take orders and fight. They will not work, eat, or rest. -->
