  <!-- EN: Fire at will -->
  <CommandFireAtWillLabel>Стреляй по усмотрение</CommandFireAtWillLabel>
  <!-- EN: Toggle fire at will.\n\nWhen disabled, this character will not automatically shoot at ranged targets. Does not affect melee combat. -->
